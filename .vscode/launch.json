  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceRoot}/dist/example.js",
      "cwd": "${workspaceRoot}",
      "outDir": "${workspaceRoot}/dist",
      "preLaunchTask": "build",
      "sourceMaps": true,
      "console": "externalTerminal",
      "env": {
        "DEBUG": "skedgy:*"
      }
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858,
      "outFiles": [],
      "sourceMaps": true
    }
  ]
}